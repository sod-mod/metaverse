{
  "name": "multiverse-wiki",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "parse-data": "node tools/parsers/cli.cjs --source=../package --output=data/extracted --verbose",
    "datamine-sprites": "node tools/sprites/datamineHeroSprites.cjs",
    "copy-sprites": "node tools/sprites/copySpriteSheets.cjs",
    "setup-sprites": "npm run datamine-sprites && npm run copy-sprites",
    "extract-icons": "node tools/sprites/extractHeroIconsImproved.cjs",
    "extract-skill-icons": "node tools/sprites/extractSkillIcons.cjs",
    "generate-i18n": "node tools/i18n/generateI18nFromData.cjs",
    "upload-images": "node tools/upload-images.cjs",
    "upload-images:dry-run": "node tools/upload-images.cjs --dry-run"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "i18next": "^25.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.2.4",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.8"
  }
}
